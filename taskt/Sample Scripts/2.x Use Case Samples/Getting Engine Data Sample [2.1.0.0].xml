<?xml version="1.0" encoding="utf-8"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Commands>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="1" PauseBeforeExeucution="false" v_Comment="Engine Context example which shows how to output engine data" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="2" PauseBeforeExeucution="false" v_Comment="Get Engine Context which contains engine information" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="VariableCommand" CommandName="VariableCommand" IsCommented="false" SelectionName="Set Variable" DefaultPause="250" LineNumber="3" PauseBeforeExeucution="false" CommandEnabled="true" v_userVariableName="vEngineData" v_Input="{taskt.EngineContext}" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="4" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="{vEngineData}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="5" PauseBeforeExeucution="false" v_Comment="Show Current Variables" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="6" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="Showing Variable List: {vEngineData =&gt; $.VariableList}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="7" PauseBeforeExeucution="false" v_Comment="Show Current App Instances" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="8" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="Showing App Instances: {vEngineData =&gt; $.AppInstances}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="9" PauseBeforeExeucution="false" v_Comment="Show Engine Setting" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="10" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="Create missing variables during execution setting: {vEngineData =&gt; $.engineSettings.CreateMissingVariablesDuringExecution}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="11" PauseBeforeExeucution="false" v_Comment="Write To File" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="WriteTextFileCommand" CommandName="WriteTextFileCommand" IsCommented="false" SelectionName="Write To File" DefaultPause="250" LineNumber="12" PauseBeforeExeucution="false" CommandEnabled="true" v_FilePath="{Folder.Desktop}\outputSample.json" v_TextToWrite="{vEngineData}" v_Overwrite="Overwrite" />
    </ScriptAction>
  </Commands>
  <Variables>
    <ScriptVariable>
      <VariableName>vEngineData</VariableName>
      <VariableValue xsi:type="xsd:string"></VariableValue>
    </ScriptVariable>
  </Variables>
</Script>