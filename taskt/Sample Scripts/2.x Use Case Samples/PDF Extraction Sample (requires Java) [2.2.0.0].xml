<?xml version="1.0" encoding="utf-8"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Commands>
    <ScriptAction>
      <ScriptCommand xsi:type="PDFTextExtractionCommand" CommandName="PDFTextExtractionCommand" IsCommented="false" SelectionName="PDF Extraction" DefaultPause="0" LineNumber="1" PauseBeforeExeucution="false" CommandEnabled="true" v_FilePath="Path\To\PDF.pdf" v_applyToVariableName="vPDFResult" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="0" LineNumber="2" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="{vPDFResult}" v_AutoCloseAfter="0" />
    </ScriptAction>
  </Commands>
  <Variables>
    <ScriptVariable>
      <VariableName>vPDFResult</VariableName>
      <VariableValue xsi:type="xsd:string"></VariableValue>
    </ScriptVariable>
  </Variables>
</Script>