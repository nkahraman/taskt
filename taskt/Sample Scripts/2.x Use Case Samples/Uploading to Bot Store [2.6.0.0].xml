<?xml version="1.0" encoding="utf-8"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Commands>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="1" PauseBeforeExeucution="false" v_Comment="Bot Store allows workers to upload/retrieve/share data." CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="2" PauseBeforeExeucution="false" v_Comment="Data is uploaded by using a Key and Value pair concept." CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="3" PauseBeforeExeucution="false" v_Comment="Note: Writing to an existing key will overwrite the data" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="4" PauseBeforeExeucution="false" v_Comment="Test uploading a sample" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="UploadDataCommand" CommandName="UploadDataCommand" IsCommented="false" SelectionName="Upload Data" DefaultPause="0" LineNumber="5" PauseBeforeExeucution="false" CommandEnabled="true" v_KeyName="test" v_InputValue="The logged time was {DateTime.Now}" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="6" PauseBeforeExeucution="false" v_Comment="Test retrieving uploaded sample value" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="GetDataCommand" CommandName="GetDataCommand" IsCommented="false" SelectionName="Get Data" DefaultPause="0" LineNumber="7" PauseBeforeExeucution="false" CommandEnabled="true" v_KeyName="test" v_DataOption="Retrieve Value" v_applyToVariableName="vOut" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="8" PauseBeforeExeucution="false" v_Comment="Show Stored Result" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="0" LineNumber="9" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="{vOut}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="10" PauseBeforeExeucution="false" v_Comment="Note we can also return the entire record with all data" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="GetDataCommand" CommandName="GetDataCommand" IsCommented="false" SelectionName="Get Data" DefaultPause="0" LineNumber="11" PauseBeforeExeucution="false" CommandEnabled="true" v_KeyName="test" v_DataOption="Retrieve Entire Record" v_applyToVariableName="vOut" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="0" LineNumber="12" PauseBeforeExeucution="false" v_Comment="Show Stored Result" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="0" LineNumber="13" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="{vOut}" v_AutoCloseAfter="0" />
    </ScriptAction>
  </Commands>
  <Variables>
    <ScriptVariable>
      <VariableName>vOut</VariableName>
      <VariableValue xsi:type="xsd:string"></VariableValue>
    </ScriptVariable>
  </Variables>
</Script>