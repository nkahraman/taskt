<?xml version="1.0" encoding="utf-8"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Commands>
    <ScriptAction>
      <ScriptCommand xsi:type="VariableCommand" CommandName="VariableCommand" IsCommented="false" SelectionName="Set Variable" DefaultPause="0" LineNumber="1" PauseBeforeExeucution="false" CommandEnabled="true" v_userVariableName="vSavePath" v_Input="{Folder.Documents}\test123.xlsx" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="0" LineNumber="2" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="{vSavePath}" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelCreateApplicationCommand" CommandName="ExcelOpenApplicationCommand" IsCommented="false" SelectionName="Create Excel Application" DefaultPause="0" LineNumber="3" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelOpenWorkbookCommand" CommandName="ExcelOpenWorkbookCommand" IsCommented="false" SelectionName="Open Workbook" DefaultPause="0" LineNumber="4" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" v_FilePath="D:\Dropbox\Environment Folders\Documents\test123.xlsx" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelSetCellCommand" CommandName="ExcelSetCellCommand" IsCommented="false" SelectionName="Set Cell" DefaultPause="0" LineNumber="5" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" v_TextToSet="TEST" v_ExcelCellAddress="A5" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelSaveAsCommand" CommandName="ExcelSaveAsCommand" IsCommented="false" SelectionName="Save Workbook As" DefaultPause="0" LineNumber="6" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" v_FileName="{vSavePath}" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelSetCellCommand" CommandName="ExcelSetCellCommand" IsCommented="false" SelectionName="Set Cell" DefaultPause="0" LineNumber="7" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" v_TextToSet="More Data" v_ExcelCellAddress="B2" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="ExcelSaveCommand" CommandName="ExcelSaveCommand" IsCommented="false" SelectionName="Save Workbook" DefaultPause="0" LineNumber="8" PauseBeforeExeucution="false" CommandEnabled="true" v_InstanceName="default" />
    </ScriptAction>
  </Commands>
  <Variables>
    <ScriptVariable>
      <VariableName>vSavePath</VariableName>
      <VariableValue xsi:type="xsd:string"></VariableValue>
    </ScriptVariable>
  </Variables>
</Script>