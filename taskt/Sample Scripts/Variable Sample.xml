<?xml version="1.0" encoding="utf-8"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Commands>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="1" PauseBeforeExeucution="false" v_Comment="Apply text to variable" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="VariableCommand" CommandName="VariableCommand" IsCommented="false" SelectionName="Set Variable" DefaultPause="250" LineNumber="2" PauseBeforeExeucution="false" CommandEnabled="true" v_userVariableName="vText" v_Input="The cow, jumped over, the moon!" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="3" PauseBeforeExeucution="false" v_Comment="show text to user" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="4" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="[vText]" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="5" PauseBeforeExeucution="false" v_Comment="show a system variable" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="MessageBoxCommand" CommandName="MessageBoxCommand" IsCommented="false" SelectionName="Show Message" DefaultPause="250" LineNumber="6" PauseBeforeExeucution="false" CommandEnabled="true" v_Message="The current time is [DateTime.Now]" v_AutoCloseAfter="0" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="CommentCommand" CommandName="CommentCommand" IsCommented="false" SelectionName="Add Code Comment" DefaultPause="250" LineNumber="7" PauseBeforeExeucution="false" v_Comment="write variable text into notepad" CommandEnabled="true" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="StartProcessCommand" CommandName="StartProcessCommand" IsCommented="false" SelectionName="Start Process" DefaultPause="250" LineNumber="8" PauseBeforeExeucution="false" CommandEnabled="true" v_ProgramName="notepad" />
    </ScriptAction>
    <ScriptAction>
      <ScriptCommand xsi:type="SendKeysCommand" CommandName="SendKeysCommand" IsCommented="false" SelectionName="Send Keystrokes" DefaultPause="250" LineNumber="9" PauseBeforeExeucution="false" CommandEnabled="true" v_WindowName="Current Window" v_TextToSend="[vText]" />
    </ScriptAction>
  </Commands>
  <Variables>
    <ScriptVariable>
      <currentPosition>0</currentPosition>
      <variableName>vText</variableName>
    </ScriptVariable>
  </Variables>
</Script>